#!../build/LuaJIT-2.0.0-beta10/src/luajit
local results = {}
for line in io.popen("ls"):lines() do
	if string.sub(line,-2) == ".t" then
		local e = { k = line }
		table.insert(results,e)
		if os.execute("../terra "..line) ~= 0 then
			e.v = "FAIL"
		else
			e.v = "pass"
		end
	end
end

for i,e in ipairs(results) do
	print(i,e.v,e.k)
end